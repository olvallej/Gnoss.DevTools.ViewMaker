          rel="stylesheet"
          class="styleForCkEditorPreview"
          href="https://fonts.googleapis.com/icon?family=Material+Icons|Material+Icons+Outlined"
          media="all" />

    <!-- FONT AWESOME -->
    <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css"
          class="styleForCkEditorPreview"
          rel="stylesheet" />

    <!-- JQUERY UI (Datepicker) -->
    <link type="text/css"
          class="styleForCkEditorPreview"
          rel="stylesheet"
          href="https://code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css" />


<!-- BOOTSTRAP 4 + PROPELLER -->
    <link rel="stylesheet"
          class="styleForCkEditorPreview"
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/bootstrap/bootstrap.min.css?v=@Html.GetVersion()"
          type="text/css" />
    <link rel="stylesheet"
          class="styleForCkEditorPreview"
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/propeller/css/propeller.min.css?v=@Html.GetVersion()"
          type="text/css" />

    <!-- LIGHTGALLERY -->
    <link type="text/css"
          rel="stylesheet"
