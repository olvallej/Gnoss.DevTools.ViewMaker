    <!-- PRELOADER MATERIALIZE -->
    <link type="text/css"
          rel="stylesheet"
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/spinner-material/spinner-material.css?v=@Html.GetVersion()" />

    <!-- DATATABLES -->    
    <link rel="stylesheet" 
          type="text/css"
          href="https://cdn.datatables.net/v/bs4/dt-1.11.3/b-2.1.1/b-html5-2.1.1/fc-4.0.1/fh-3.2.1/r-2.2.9/sc-2.0.5/sl-1.3.4/datatables.min.css">

    <!-- SELECT2 -->
    <link type="text/css"
          rel="stylesheet"          
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/select2/css/select2.min.css?v=@Html.GetVersion()" />

    <!-- Toastr -->
    <link type="text/css"
          rel="stylesheet"
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/toastr/toastr.min.css?v=@Html.GetVersion()" />

    <!-- Simple Donut -->
    <link type="text/css"
          rel="stylesheet"
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/simple-donut/simple-donut.css?v=@Html.GetVersion()" />

    <!-- Owl Carousel -->
    <link type="text/css"
          rel="stylesheet"
          href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/owl-carousel/css/owl.carousel.min.css?v=@Html.GetVersion()" />
      
    <!-- PERSONALIZACION TEMA -->
    <link type="text/css"
          class="styleForCkEditorPreview"
          rel="stylesheet"
          href="@Html.GetBaseUrlStatic()/responsive/theme/theme.css?v=@Html.GetVersion()" />

    <!-- DatePicker -->
    <link type="text/css"
          class="styleForCkEditorPreview"
          rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css" />

    <!-- BUSYLOAD -> VISTAS LOADING -->
    <link type="text/css"
      rel="stylesheet"
      href="@Html.GetBaseUrlStatic()/responsive/theme/libraries/busy-load/app.min.css?v=@Html.GetVersion()" />

    <!-- Visualizador Prism para Code en TinyMCE -->
    <link type="text/css"
          rel="stylesheet"
          href="@Html.GetBaseUrlStatic()/responsive/theme/prism/prism.css?v=@Html.GetVersion()" />

@if (Html.GetListaCSS() != null)
{
    foreach (string css in Html.GetListaCSS())
    {
        <link media="screen" type="text/css" rel="stylesheet" href="@css" />
    }
}

